generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  event_id   String   @id @default(uuid())
  session_id String   @default(uuid())
  event_name String
  url        String
  user_ip    String
  utm_source String?
  referrer   String?
  user_agent String
  timestamp  DateTime @default(now())
}
